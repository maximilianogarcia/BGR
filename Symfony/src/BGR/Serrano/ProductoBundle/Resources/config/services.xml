<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

  <parameters>
    <parameter key="presentacion_service.class">BGR\Serrano\ProductoBundle\Service\PresentacionService</parameter>
    <parameter key="producto_service.class">BGR\Serrano\ProductoBundle\Service\ProductoService</parameter>
   </parameters>  
    <services>
	    <service id="presentacion_service" class="%presentacion_service.class%"></service>
	    <service id="producto_service" class="%producto_service.class%"></service>
	</services>
</container>
